/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in= sel, out= notSel);
    Nand(a= a, b= notSel, out= t0);
    Nand(a= b, b= sel, out= t1);
    Nand(a= t0, b= t1, out= out);
}